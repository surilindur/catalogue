{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "tsconfigRootDir": ".",
    "project": [
      "./tsconfig.json"
    ]
  },
  "extends": [
    "@rubensworks"
  ],
  "rules": {
    "unicorn/filename-case": "off",
    "import/group-exports": "off",
    "import/no-nodejs-modules": "off"
  }
}