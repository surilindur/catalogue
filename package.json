  "dependencies": {
    "asynciterator": "^3.0.0",
    "bloem": "^0.2.0",
    "componentsjs": "^5.0.0",
    "n3": "^1.0.0",
    "rdf-data-factory": "^1.0.0",
    "rdf-dereference": "^2.0.0",
    "yargs": "^17.0.0"
  },
  "devDependencies": {
    "@rdfjs/types": "*",
    "@rubensworks/eslint-config": "^2.0.0",
    "@types/bloem": "^0.2.0",
    "@types/n3": "^1.0.0",
    "componentsjs-generator": "^3.0.0",
    "eslint": "8.48.0",
    "typescript": "^5.0.0"
  }
